<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.A4.oplev">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity_Main" />
        <activity android:name=".Activity_Profile" />
        <activity
            android:name=".Activity_Ini"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity_U_Settings" />
        <activity android:name=".Activity_Search_Filter" />
        <activity android:name=".Activity_Create_Event" />
        <activity android:name=".Activity_Likeside" />
        <activity android:name=".Activity_Chat" />
    </application>

</manifest>