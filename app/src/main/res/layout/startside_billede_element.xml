<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/startside_frag_"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/startSide_Billede"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/icecrim" />

    <ImageView
        android:id="@+id/startside_frag_pp"
        android:layout_width="109dp"
        android:layout_height="98dp"
        app:srcCompat="@drawable/_profiltest" />

    <TextView
        android:id="@+id/startside_frag_headline"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginEnd="143dp"
        android:layout_marginRight="143dp"
        android:layout_toEndOf="@+id/startside_frag_pp"
        android:layout_toRightOf="@+id/startside_frag_pp"
        android:text="Headline" />

    <TextView
        android:id="@+id/startside_frag_withWho"
        android:layout_width="143dp"
        android:layout_height="17dp"
        android:layout_below="@+id/startside_frag_headline"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="13dp"
        android:layout_marginLeft="13dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="146dp"
        android:layout_marginRight="146dp"
        android:layout_toEndOf="@+id/startside_frag_pp"
        android:layout_toRightOf="@+id/startside_frag_pp"
        android:text="TextView" />
</RelativeLayout>