<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TableLayout
                android:id="@+id/tableLayout"
                android:layout_width="344dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp">

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView4"
                        android:layout_width="300px"
                        android:layout_height="300px"
                        app:srcCompat="@drawable/obama" />

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="300px"
                        android:layout_height="300px"
                        app:srcCompat="@drawable/obama" />

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="300px"
                        android:layout_height="300px"
                        app:srcCompat="@drawable/obama" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView8"
                        android:layout_width="300px"
                        android:layout_height="300px"
                        app:srcCompat="@drawable/obama" />

                    <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="300px"
                        android:layout_height="300px"
                        app:srcCompat="@drawable/obama" />

                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="300px"
                        android:layout_height="300px"
                        app:srcCompat="@drawable/obama" />
                </TableRow>

            </TableLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_margin="16dp"
                android:divider="@drawable/divider"
                android:orientation="vertical"
                android:showDividers="middle">

                <TextView
                    android:id="@+id/label_About"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text='"Om mig' />

                <EditText

                    android:id="@+id/textBox_About"
                    android:layout_width="match_parent"
                    android:layout_height="82dp"
                    android:background="#FFFFFF"
                    android:bottomLeftRadius="15dp"
                    android:bottomRightRadius="15dp"
                    android:ems="10"
                    android:inputType="textPersonName"

                    android:topLeftRadius="15dp"
                    android:topRightRadius="15dp" />

                <TextView
                    android:id="@+id/label_City"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="By" />

                <EditText
                    android:id="@+id/textBox_City"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:ems="10"
                    android:inputType="textPersonName" />

                <TextView
                    android:id="@+id/label_Job"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Job" />

                <EditText
                    android:id="@+id/textBoks_Job"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:ems="10"
                    android:inputType="textPersonName" />

                <TextView
                    android:id="@+id/label_edu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Udannelse " />

                <EditText
                    android:id="@+id/textBox_edu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#FFFFFF"
                    android:ems="10"
                    android:inputType="textPersonName" />

                <TextView
                    android:id="@+id/label_Instagram"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Instagram" />

                <Button
                    android:id="@+id/button_ConnectInstagram"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Connect" />
                zÂ½
            </LinearLayout>

        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>