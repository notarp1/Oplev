<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/eventItem_Billede"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/icecrim" />

    <View
        android:id="@+id/boxyea"
        android:layout_width="0dp"
        android:layout_height="55dp"

        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:background="@drawable/beskedbackground2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/view12"
        app:layout_constraintTop_toTopOf="@+id/eventItem_Billede" />

    <ImageView
        android:id="@+id/eventItem_pp"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_profiltest" />


    <View
        android:id="@+id/view12"
        android:layout_width="123dp"
        android:layout_height="123dp"
        android:layout_marginStart="9dp"
        android:layout_marginLeft="9dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/circle1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/evntItem_withWho"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginLeft="60dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="3dp"
        android:maxLines="1"
        android:minLines="1"
        android:text="med Kenneth"
        android:textColor="#A4E8E0"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/boxyea"
        app:layout_constraintEnd_toStartOf="@+id/eventitem_distancetext"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/boxyea" />

    <TextView
        android:id="@+id/eventitem_distancetext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:autoSizeMaxTextSize="100sp"
        android:autoSizeMinTextSize="12sp"
        android:autoSizeTextType="uniform"
        android:gravity="right"
        android:text="150 km"
        android:textColor="#A4E8E0"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/boxyea"
        app:layout_constraintStart_toEndOf="@+id/eventitem_Headline"
        app:layout_constraintTop_toTopOf="@+id/boxyea" />

    <TextView
        android:id="@+id/eventitem_Headline"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="55dp"
        android:layout_marginLeft="55dp"
        android:layout_marginTop="5dp"
        android:autoSizeMaxTextSize="100sp"
        android:autoSizeMinTextSize="12sp"
        android:autoSizeTextType="uniform"
        android:inputType="textNoSuggestions"
        android:maxLines="1"
        android:minLines="1"
        android:text="Leverposteg med mig"
        android:textColor="#A4E8E0"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/eventitem_distancetext"
        app:layout_constraintStart_toStartOf="@+id/boxyea"
        app:layout_constraintTop_toTopOf="@+id/boxyea" />

    <TextView
        android:id="@+id/txt_date"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginBottom="3dp"
        android:autoSizeMaxTextSize="100sp"
        android:autoSizeMinTextSize="8sp"
        android:autoSizeTextType="uniform"
        android:maxLines="1"
        android:minLines="1"
        android:text="08/09/2020"
        android:textColor="#A4E8E0"
        android:textSize="12sp"
        app:layout_constraintBottom_toBottomOf="@+id/boxyea"
        app:layout_constraintEnd_toEndOf="@+id/boxyea" />

</androidx.constraintlayout.widget.ConstraintLayout>
