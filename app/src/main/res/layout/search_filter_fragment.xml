    <TextView
        android:id="@+id/textViewbaground2"
        android:layout_width="411dp"
        android:layout_height="30dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAktiviteret" />

    <Switch
        android:id="@+id/switchALLActivities"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="36dp"
        android:layout_marginRight="36dp"
        android:checked="true"
        app:layout_constraintBottom_toBottomOf="@+id/textViewbaground2"
        app:layout_constraintEnd_toEndOf="@+id/textViewbaground2"
        app:layout_constraintTop_toTopOf="@+id/textViewbaground2"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
